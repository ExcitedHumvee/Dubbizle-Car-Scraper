{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend for Dubbizle Car Scraper",
  "main": "index.js",
  "scripts": {
    "start": "nodemon index.js",
    "scrape": "node scraper/scraper.js && node scraper/save-to-db.js && node scraper/countCars.js && node export-all-cars.js && node update-index-with-allcars.js",
    "scrape:test": "node scraper/test.js",
    "db:save": "node scraper/save-to-db.js && node scraper/countCars.js",
    "prisma:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "test:api": "node test_api.js",
    "studio": "npx prisma studio",
    "export:all": "node export-all-cars.js",
    "sync:index": "node update-index-with-allcars.js",
    "db:vacuum": "npx prisma db execute --file vacuum.sql"
  },
  "keywords": [],
